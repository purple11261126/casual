    "import pandas as pd\n",
    "\n",
    "csv = './mining.csv'\n",
    "file = pd.read_csv(csv)\n",
    "print(file.head())\n",
    "print()\n",
    "\n",
    "file = file.groupby('school_num').sum().sort_values(by=('results'), ascending=False)\n",
    "print(file)\n",
    "file.loc[file.idxmax()]  # idxmax() 不能获取多个最大值"
   ]
